{
  "name": "remark-pdf",
  "version": "0.0.18",
  "description": "remark plugin to compile markdown to pdf.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "default": "./lib/index.js"
    },
    "./node": {
      "types": "./lib/node.d.ts",
      "require": "./lib/node.cjs",
      "default": "./lib/node.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "tsc": "tsc -p . --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "typedoc": "typedoc",
    "prepublishOnly": "npm run typedoc && rimraf lib && npm run build"
  },
  "dependencies": {
    "@types/pdfmake": "^0.2.12",
    "mdast-util-definitions": "^6.0.0",
    "pdfmake": "^0.2.20",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.0.0",
    "@storybook/react-vite": "10.1.8",
    "@types/file-saver": "^2.0.7",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/mdast": "^4.0.3",
    "@types/react": "^19.2.7",
    "@types/unist": "^3.0.0",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.11.1",
    "jest-image-snapshot": "^6.5.1",
    "lodash.debounce": "^4.0.8",
    "pdf-to-img": "^5.0.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-is": "^19.2.1",
    "react-simple-code-editor": "^0.14.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "storybook": "10.1.8",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "unified": "^11.0.4",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/remark-pdf.git"
  },
  "keywords": [
    "unist",
    "remark",
    "mdast",
    "markdown",
    "pdf"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/remark-pdf/issues"
  },
  "homepage": "https://github.com/inokawa/remark-pdf#readme"
}
